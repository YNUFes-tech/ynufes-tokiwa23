    "eslint-plugin-prettier": "^5.0.0",
    "nuxt": "^3.6.5",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "typescript": "^5.2.2"
